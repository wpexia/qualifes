<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.qualifies.app"
          android:versionCode="1"
          android:versionName="1.0">

    <uses-sdk
            android:minSdkVersion="15"
            android:targetSdkVersion="22"/>

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@drawable/ic_launcher"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.NoTitleBar">
        <activity
                android:name=".ui.RegisterActivity"
                android:label="@string/register_name">
        </activity>
        <activity
                android:name=".ui.ProtocolActivity"
                android:label="@string/protocol_name">
        </activity>
        <activity
                android:name=".ui.personal.PersonalActivity"
                android:label="@string/wo_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>
        <activity android:name="com.qualifies.app.qualifieslife.MainActivity"
                  android:label="@string/app_name">

        </activity>
        <activity android:name=".ui.LoginActivity"/>
        <activity android:name=".ui.personal.HistoryActivity"/>
        <activity android:name=".ui.personal.FollowActivity"/>
        <activity android:name=".ui.personal.SettingActivity"/>
        <activity android:name=".ui.personal.RealNameActivity"/>
        <activity android:name=".ui.personal.PositionActivity"/>
        <activity android:name=".ui.personal.AddPositionActivity"/>
        <activity android:name=".ui.personal.MoneyActivity"/>
        <activity
                android:name="com.qualifies.app.qualifieslife.HomePageActivity"
                android:label="@string/title_activity_home_page"/>
        <activity android:name="com.qualifies.app.qualifieslife.GoodDetailActivity"/>
        <activity
                android:name="com.qualifies.app.qualifieslife.GoodSpecActivity"
                android:label="@string/title_activity_good_spec">
        </activity>
        <activity
                android:name="com.qualifies.app.qualifieslife.HomeSearchActivity"
                android:label="@string/title_activity_home_search"/>
        <activity
                android:name="com.qualifies.app.qualifieslife.HomeSearchResultActivity"
                android:label="@string/title_activity_home_search_result"/>
    </application>

</manifest>
