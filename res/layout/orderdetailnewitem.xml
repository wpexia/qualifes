<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:background="@color/white"
                android:minHeight="78dp"
                android:layout_marginBottom="20dp"
                android:layout_height="78dp" android:id="@+id/item">

    <ImageView
            android:layout_width="60dp"
            android:layout_marginTop="9dp"
            android:layout_marginLeft="15dp"
            android:src="@drawable/img_empty"
            android:layout_height="60dp"
            android:id="@+id/imageView8" android:layout_below="@+id/textView6" android:layout_alignLeft="@+id/textView6"
            android:layout_alignStart="@+id/textView6"/>
    <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:lines="2"
            android:textSize="12sp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="50dp"
            android:textColor="#6e6e6e"
            android:id="@+id/textView9" android:layout_alignTop="@+id/imageView8"
            android:layout_toRightOf="@+id/imageView8" android:layout_toEndOf="@+id/imageView8"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:textSize="12sp"
            android:textColor="#6e6e6e"
            android:lines="1"
            android:layout_marginTop="6dp"
            android:id="@+id/textView15" android:layout_below="@+id/textView9" android:layout_alignLeft="@+id/textView9"
            android:layout_alignStart="@+id/textView9"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_in"
            android:id="@+id/imageView9"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true" android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"/>
</RelativeLayout>